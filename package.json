{
  "name": "express_hacker_news",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "clean": "rimraf dist",
    "lint": "eslint . --ext=.ts",
    "format:check": "prettier --check .",
    "format": "prettier --write .",
    "dev": "tsc-watch --onSuccess \"node ./bin/www\"",
    "build": "tsc",
    "build:start": "npm run build & npm run start",
    "test": "mocha -r ts-node/register test/**/*.spec.ts --recursive",
    "test:coverage": "nyc npm run test",
    "test:parallel": "mocha --parallel -r ts-node/register test/**/*.spec.ts --recursive",
    "test_only_file": "mocha -r ts-node/register test/hacker_new_service.spec.ts",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:run:arg": "typeorm migration:run -d dist/data-source.js"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "compression": "^1.7.4",
    "cookie-parser": "~1.4.5",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "dotenv": "^10.0.0",
    "express": "^4.18.1",
    "helmet": "^4.6.0",
    "http-errors": "^2.0.0",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.2",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.9",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.5.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/chai": "^4.3.3",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/http-errors": "^1.8.2",
    "@types/mocha": "^9.1.1",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.11.59",
    "@types/node-cron": "^3.0.4",
    "@types/sinon": "^10.0.13",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "chai": "^4.3.6",
    "eslint": "8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "sinon": "^14.0.0",
    "supertest": "^6.2.4",
    "ts-node": "^10.9.1",
    "tsc-watch": "^5.0.3",
    "typescript": "4.7.4"
  }
}
